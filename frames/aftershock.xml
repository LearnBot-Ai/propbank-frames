<!DOCTYPE frameset SYSTEM "frameset.dtd">
<frameset>
  <predicate lemma="aftershock">
    <roleset id="aftershock.01" name="secondary earthquake, coming after the main one">
      <aliases>
        <alias framenet="" pos="n" verbnet="">aftershock</alias>
      </aliases>
      <note>AFTERSHOCK-N NOTES: Added by Julia for earthquakes.</note>
      <roles>
        <note>The "VSP" function tags on 'death/injury toll' and 'damages/cost' correspond to the RED cause relation.</note>
        <role descr="ground shaken (plates, region, epicenter)" f="ppt" n="1"/>
        <role descr="magnitude, generally Richter scale" f="ext" n="2"/>
        <role descr="death/injury toll" f="vsp" n="3"/>
        <role descr="damages/cost" f="vsp" n="4"/>
      </roles>
      <example name="aftershock-n" src="" type="">
        <text> Fukushima hit by 5.7-magnitude aftershock. </text>
        <arg f="" n="2">5.7-magnitude</arg>
        <rel f="">aftershock</rel>
      </example>
    </roleset>
  </predicate>
</frameset>
