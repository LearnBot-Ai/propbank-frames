<frameset>
  <predicate lemma="quadrangulate">
    <roleset id="quadrangulate.01" name="pinpoint the location of using 4 points (as opposed to 3 in triangulation)">
      <aliases>
        <alias pos="v">quadrangulate</alias>
        <alias pos="n">quadrangulation</alias>
      </aliases>
      <roles>
        <role descr="quadrangulator" f="PAG" n="0"/>
        <role descr="location searched for" f="PPT" n="1"/>
        <role descr="based on, known info" f="MNR" n="2"/>
      </roles>
      <usagenotes>
        <usage resource="PropBank" version="3.0" inuse="+"/>
        <usage resource="AMR" version="3.0" inuse="-"/>
      </usagenotes>
      <example src="" name="quadrangulate-v">
        <text>With enough data it will, well, "quadrangulate" the location of the Temporal Exterminator, the actual firing mechanism.</text>
        <rel relloc="6">"quadrangulate"</rel>
        <arg f="ADV" n="m" start="0" end="2">With enough data</arg>
        <arg f="" n="0" start="3" end="3">it</arg>
        <arg f="MOD" n="m" start="4" end="4">will,</arg>
        <arg f="ADV" n="m" start="5" end="5">well,</arg>
        <arg f="" n="1" start="7" end="16">the location of the Temporal Exterminator, the actual firing mechanism.</arg>
      </example>
      <note>QUADRANGULATE-V NOTES: Added by Julia.</note>
      <note>QUADRANGULATION-N NOTES: Added by Julia.</note>
    </roleset>
    <roleset id="quadrangulate.02" name="divide into quadrangles">
      <aliases>
        <alias pos="v">quadrangulate</alias>
        <alias pos="n">quadrangulation</alias>
      </aliases>
      <roles>
        <role descr="quadrangulator" f="PAG" n="0"/>
        <role descr="region divided into quadrangles" f="PPT" n="1"/>
        <role descr="quadrangle side length measurement" f="VSP" n="2"/>
