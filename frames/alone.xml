<!DOCTYPE frameset SYSTEM "frameset.dtd">
<frameset>
  <predicate lemma="alone">
    <roleset id="alone.01" name="isolated, unaccompanied, unique">
      <aliases>
        <alias framenet="Accompaniment" pos="j" verbnet="">alone</alias>
      </aliases>
      <note>ALONE-J NOTES: Added by Julia based on BOLT-4,5. (from alone.01-j)</note>
      <roles>
        <role descr="loner" f="pag" n="1"/>
        <role descr="domain in which arg1 is unique" f="ppt" n="2"/>
      </roles>
      <example name="alone-j: both args" src="BOLT-4,5" type="">
        <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
        <text>It 's frustrating , you 're not alone in feeling the way you do .</text>
        <arg f="" n="1">you</arg>
        <arg f="neg" n="m">not</arg>
        <rel f="">alone</rel>
        <arg f="" n="2">in feeling the way you do</arg>
      </example>
    </roleset>
  </predicate>
  
  <predicate lemma="leave_alone">
    <roleset id="leave_alone.01" name="stop bothering">
      <aliases>
        <alias framenet="" pos="m" verbnet="">leave_alone</alias>
        <alias framenet="" pos="m" verbnet="">leaving_alone</alias>
      </aliases>
      <mwe alias="leave_alone">
        <map form="source">
            (A / leave-14 
                :arg0 (n / NARG0)
                :arg1 (B / alone-01
                    :arg1 (n1 / NARG1)))
        </map>
        <map form="target">
            (A / bother-01 :polarity -
                :arg0 (n / NARG0)
                :arg1 (n1 / NARG1))
        </map>
        <token id="A" token="leave" arg="" pos="VB" dep=""/>
        <token id="B" token="alone" arg="" pos="JJ" dep="acomp/A"/>
      </mwe>
      <note>LEAVE_ALONE-M NOTES: Added by Julia and Tim.</note>
      <roles>
        <role descr="bothersome entity" f="pag" n="0"/>
        <role descr="entity who doesn't want to be bothered" f="ppt" n="1"/>
      </roles>
      <example name="leave_alone-m" src="" type="">
        <text>Now *PRO* go away then, and leave me alone. I don't want any more of your meretricious persiflage.</text>
        <arg f="dis" n="m">Now</arg>
        <arg f="" n="0">*PRO*</arg>
        <rel f="">[leave][alone]</rel>
        <arg f="" n="1">me</arg>
      </example>
      <example name="leave_alone-m" src="" type="">
        <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
        <text>Leave the Marines alone.</text>
        <rel f="">[leave][alone]</rel>
        <arg f="" n="1">the Marines</arg>
      </example>
    </roleset>
  </predicate>
</frameset>
