<!DOCTYPE frameset SYSTEM "frameset.dtd">
<frameset>
  <predicate lemma="crossbreed">
    <roleset id="crossbreed.01" name="breeding different varieties">
      <aliases>
        <alias framenet="" pos="v" verbnet="">crossbreed</alias>
        <alias framenet="" pos="v" verbnet="">cross-breed</alias>
        <alias framenet="" pos="v" verbnet="">cross_breed</alias>
        <alias framenet="" pos="n" verbnet="">crossbreeding</alias>
        <alias framenet="" pos="n" verbnet="">cross-breeding</alias>
