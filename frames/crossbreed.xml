        <rel f="">[CROSS]-[BRED]</rel>
        <arg f="" n="1">PLANTS</arg>
        <arg f="" n="3">to produce the billion-dollar Idaho potato</arg>
      </example>
      <example name="cross_breed-v: tasty cows, all arguments" src="web" type="">
        <inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
        <text>He cross bred Longhorn with Hereford cattle to produce excellent beef animals.</text>
        <arg f="" n="0">He</arg>
        <rel f="">[cross][bred]</rel>
        <arg f="" n="1">Longhorn</arg>
        <arg f="" n="2">with Hereford</arg>
        <arg f="" n="3">to produce excellent beef animals</arg>
      </example>
    </roleset>
  </predicate>
</frameset>
