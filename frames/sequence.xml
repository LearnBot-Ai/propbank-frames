        <text>I sequenced Mary before John in order of intelligence .</text>
        <arg type="ARG0" start="?" end="?">I</arg>
        <arg type="ARG1" start="?" end="?">Mary</arg>
        <arg type="ARG2" start="?" end="?">before John</arg>
        <arg type="ARG3" start="?" end="?">in order of intelligence</arg>
        <rel relloc="1">sequenced</rel>
      </example>
