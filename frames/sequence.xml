        <alias pos="n">sequencing</alias>
        <alias pos="n">sequence</alias>
      </aliases>
      <roles>
        <role descr="arranger, agent" f="PAG" n="0"/>
        <role descr="things put in order or first element" f="PPT" n="1"/>
        <role descr="second or subsequent element" f="PPT" n="2"/>
