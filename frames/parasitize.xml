        <alias framenet="" pos="v" verbnet="">parasitize</alias>
        <alias framenet="" pos="v" verbnet="">parasitise</alias>
        <alias framenet="" pos="n" verbnet="">parasitization</alias>
        <alias framenet="" pos="n" verbnet="">parasitisation</alias>
      </aliases>
