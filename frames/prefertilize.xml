<!DOCTYPE frameset SYSTEM "frameset.dtd">
<frameset>
  <predicate lemma="prefertilize">
    <roleset id="prefertilize.01" name="add fertilizer beforehand">
      <aliases>
        <alias framenet="" pos="v" verbnet="">prefertilize</alias>
        <alias framenet="" pos="v" verbnet="">pre-fertilize</alias>
        <alias framenet="" pos="v" verbnet="">prefertilise</alias>
        <alias framenet="" pos="v" verbnet="">pre-fertilise</alias>
        <alias framenet="" pos="n" verbnet="">prefertilization</alias>
        <alias framenet="" pos="n" verbnet="">pre-fertilization</alias>
        <alias framenet="" pos="n" verbnet="">prefertilisation</alias>
        <alias framenet="" pos="n" verbnet="">pre-fertilisation</alias>
        <alias framenet="" pos="j" verbnet="">prefertilized</alias>
        <alias framenet="" pos="j" verbnet="">pre-fertilized</alias>
        <alias framenet="" pos="j" verbnet="">prefertilised</alias>
        <alias framenet="" pos="j" verbnet="">pre-fertilised</alias>
      </aliases>
      <note>PREFERTILIZE-V NOTES: Added by Julia for clearearth-biotexts.</note>
      <roles>
        <role descr="agent" f="pag" n="0"/>
        <role descr="soil, etc., which fertilizer is added to" f="ppt" n="1"/>
        <role descr="fertilizer" f="mnr" n="2"/>
      </roles>
      <example name="pre-fertilize-v" src="internet" type="">
        <text>[Some mixes]-1 are pre-fertilized *trace*-1 with enough nutrients to take a tomato plant to the 4 leaf stage.</text>
        <rel f="">pre-fertilized</rel>
        <arg f="" n="1">*trace*-1</arg>
        <arg f="" n="2">with enough nutrients to take a tomato plant to the 4 leaf stage</arg>
      </example>
    </roleset>
    <roleset id="prefertilization.02" name="before fertilization">
      <aliases>
        <alias framenet="" pos="j" verbnet="">pre-fertilization</alias>
        <alias framenet="" pos="j" verbnet="">pre-fertilisation</alias>
      </aliases>
      <note>PREFERTILIZE-V NOTES: Added by Julia for clearearth-biotexts.</note>
      <roles>
        <role descr="thing occuring prior to fertilization" f="ppt" n="1"/>
      </roles>
      <example name="pre-fertilization-j" src="clearearth-biotexts" type="">
        <text>Another potential source of bias was pre-fertilization selection, but this too can be discounted.</text>
        <rel f="">pre-fertilization</rel>
        <arg f="" n="1">selection</arg>
      </example>
    </roleset>
  </predicate>
</frameset>
